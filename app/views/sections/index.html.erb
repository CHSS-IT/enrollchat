<p id="notice"><%= notice %></p>

<h1>Sections</h1>

<table id="class-sections" class="table table-striped table-bordered table-responsive" cellspacing="0" width="100%">
  <thead>
    <tr class="bg-info">
      <th>Term</th>
      <th>Section</th>
      <th>Department</th>
      <th>Cross list group</th>
      <th>Course description</th>
      <th>Section number</th>
      <th>Title</th>
      <th>Credits</th>
      <th>Level</th>
      <th>Status</th>
      <th>Enrollment limit</th>
      <th>Actual enrollment</th>
      <th>Cross list enrollment</th>
      <th>Waitlist</th>
      <th>Comments</th>
    </tr>
  </thead>

  <tbody>
    <% @sections.each do |section| %>
      <tr>
        <td><%= section.term %></td>
        <td><%= section.section_id %></td>
        <td><%= section.department %></td>
        <td><%= section.cross_list_group %></td>
        <td><%= section.course_description %></td>
        <td><%= section.section_number %></td>
        <td><%= section.title %></td>
        <td><%= section.credits %></td>
        <td><%= section.level %></td>
        <td><%= section.status %></td>
        <td><%= section.enrollment_limit %></td>
        <td><%= section.actual_enrollment %></td>
        <td><%= section.cross_list_enrollment %></td>
        <td><%= section.waitlist %></td>
        <td>COMMENTS</td>
      </tr>
    <% end %>
  </tbody>
</table>

<script type="application/javascript">
  $(document).ready(function() {
    $('#class-sections').DataTable();
  } );
</script>
<span id="alert-wrapper"></span>
<div class="row">
  <div class="col-sm-8">
    <h1>Users</h1>

    <table class="table table-striped table-bordered table-responsive table-sm table-hover" cellspacing="0" width="100%">
      <thead>
      <tr class="bg-info">
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>User Name</th>
        <th>Admin</th>
      </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
          <td><%= user.username %></td>
          <td><%= user.admin %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
